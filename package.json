{
  "name": "sharkai-onewave",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "npm run build && next start",
    "lint": "next lint",
    "goDev": "concurrently \"npm run dev:api\" \"npm run dev:front\"",
    "go": "concurrently \"npm run prod:api\" \"npm run prod:front\"",
    "dev:api": "cd test-case/test-case/ && npm install && npm run dev",
    "dev:front": "yarn && next dev --turbopack",
    "prod:api": "cd test-case/test-case/ && npm install && npm run build && npm run start",
    "prod:front": "yarn && yarn build && yarn start",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "engines": {
    "node": "20.13.1"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.2",
    "@heroicons/react": "^2.2.0",
    "@react-spring/web": "^9.7.5",
    "@reduxjs/toolkit": "^2.7.0",
    "axios": "^1.8.4",
    "classnames": "^2.5.1",
    "concurrently": "^9.1.2",
    "framer-motion": "^12.7.4",
    "next": "15.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.55.0",
    "react-hot-toast": "^2.5.2",
    "react-redux": "^9.2.0",
    "react-select": "^5.10.1",
    "react-select-async-paginate": "^0.7.9",
    "react-spinners": "^0.16.1",
    "sass": "^1.86.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4.1.4",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.3.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.5.3",
    "tailwindcss": "^4.1.4",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
